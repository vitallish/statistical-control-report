	Journal = {Quality \& Safety in Health Care},
	Month = {10},
	Number = {5},
	Pages = {387--399},
	Publisher = {BMJ Group},
	Title = {Application of statistical process control in healthcare improvement: systematic review},
	Ty = {JOUR},
	U1 = {qc22194{$[$}PII{$]$}; 17913782{$[$}pmid{$]$}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2464970/},

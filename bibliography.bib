@article{BinJumah2012,
abstract = {This article discusses the use of quality control techniques in the improvement of the trading, finance, banking, and service industries. One such technique being used to assess performance of trading machines is statistical process control (SPC). In addition, quality management initiatives are being implemented in Hong Kong banks to improve their competitive position. Finally, Lean Six Sigma (LSS) and Total Quality Management (TQM) techniques are improving the competitiveness of banking and service industries in the U.S. Finally, some exploration into the improvement of government services using LSS and TQM practices is attempted.},
